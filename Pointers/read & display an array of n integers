#include <stdio.h> 
int main() 
{ 
    int i,n; 
    int arr[10], *ptr = arr; 
    printf("\n Enter the element of array "); 
    scanf("%d",&n); 
    printf("\n Enter the element "); 
    for(i=0;i<n;i++) 
    scanf("%d",ptr+i); 
    printf("Element emtered are :->"); 
    for(i=0;i<n;i++) 
      printf("\t %d",*(ptr+i)); 
    return 0; 
}
